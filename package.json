{
  "name": "ai-news-bot",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "engines": {
    "node": ">=24.0.0"
  },
  "scripts": {
    "init:dirs": "mkdir -p news sources .cache && [ -f sources/url_inbox.md ] || echo \"# URL Inbox\\n\" > sources/url_inbox.md && [ -f .cache/seen.json ] || echo \"{}\" > .cache/seen.json",
    "build:news": "node scripts/build_ai_news.mjs",
    "pick:week": "node scripts/pick_ai_week.mjs",
    "smoke": "node scripts/smoke.mjs",
    "start:bot": "node bot/index.mjs"
  },
  "dependencies": {
    "@mozilla/readability": "^0.5.0",
    "discord.js": "^14.15.3",
    "dotenv": "^16.4.5",
    "jsdom": "^24.1.0",
    "node-fetch": "^3.3.2",
    "iconv-lite": "^0.6.3",
    "jschardet": "^3.0.0",
    "turndown": "^7.1.3",
    "dayjs": "^1.11.13",
    "rss-parser": "^3.13.0",
    "sanitize-filename": "^1.6.3"
  }
}